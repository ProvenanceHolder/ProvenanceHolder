<?xml version="1.0" encoding="UTF-8" ?>
<xesextension name="Adaptation" prefix="adaptation" uri="http://www.xes-standard.org/adaptation.xesext">
	<!--
		The Adaptation extension specifies for events that an adaptation (insert or delete) is enacted.
	-->
	<event>
		<string key="type">
			<!--
				type of the adaptation (insert or delete)
			-->
			<alias mapping="EN" name="type"/>
		</string>
		<string key="location">
			<!--
				location of the adaptation (which activity), especially important for delete. (where)
			-->
			<alias mapping="EN" name="location"/>
		</string>
		<string key="change">
			<!--
				the actual change (what)
			-->
			<alias mapping="EN" name="change"/>
		</string>
		<string key="initiator">
			<!--
				change initiator (who)
			-->
			<alias mapping="EN" name="initiator"/>
		</string>
		<date key="timestamp">
			<!--
				The UTC time at which the change occurred. (when)
				(the time of the enactment of a change and a change itself are not necessarly the same.
			-->
			<alias mapping="EN" name="Timestamp"/>
		</date>
		<string key="reason">
			<!--
				reason for the change (why)
			-->
			<alias mapping="EN" name="reason"/>
		</string>
		<string key="impact">
			<!--
				impact of the change
			-->
			<alias mapping="EN" name="impact"/>
		</string>
	</event>
</xesextension>
